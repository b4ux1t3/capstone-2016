<Window x:Class="capstone.ReportingHome"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:capstone"
        mc:Ignorable="d"
        Title="Reporting" Height="239.098" Width="153.94" Foreground="#FF66E0E0"
        Closing="Window_Closing" ResizeMode="NoResize">
    <Grid Margin="0,10,0,0">
        <Button x:Name="Patients" Content="Patients" HorizontalAlignment="Left" Margin="10,3,0,0" VerticalAlignment="Top" Width="125" Height="30" Click="Patients_Click"/>
        <Button x:Name="Treatments" Content="Treatments" HorizontalAlignment="Left" Margin="10,56,0,0" VerticalAlignment="Top" Width="125" Height="30" Click="Treatments_Click"/>
        <Button x:Name="StaffSchedule" Content="Staff Schedule" HorizontalAlignment="Left" Margin="10,110,0,0" VerticalAlignment="Top" Width="125" Height="30" Click="StaffSchedule_Click"/>

    </Grid>
</Window>
