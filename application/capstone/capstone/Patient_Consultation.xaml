<Window x:Class="capstone.WinPatientConsult"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:capstone"
        mc:Ignorable="d"
        Title="Consultation Patient Lookup" Height="239.098" Width="499.624"
        Closing="Window_Closing">
    <Grid Margin="0,10,0,0">
        <TextBox x:Name="txtPatientID" HorizontalAlignment="Left" Height="23" Margin="95,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="365"/>
        <Label x:Name="label" Content="Patient ID:&#xD;&#xA;" HorizontalAlignment="Left" Margin="26,74,0,0" VerticalAlignment="Top" Height="23"/>
        <Button x:Name="btnSubmit" Content="Submit" HorizontalAlignment="Left" Margin="196,168,0,0" VerticalAlignment="Top" Width="117" Click="btnSubmit_Click"/>
        <Button x:Name="btnBrowse" Content="Browse" HorizontalAlignment="Left" Margin="217,112,0,0" VerticalAlignment="Top" Width="75" Click="btnBrowse_Click"/>

    </Grid>
</Window>
