<Window x:Class="capstone.WinLogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:capstone"
        mc:Ignorable="d"
        Title="Log In" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="txtStaffID" HorizontalAlignment="Left" Height="23" Margin="75,158,0,0" TextWrapping="Wrap" Text="Enter Staff ID" VerticalAlignment="Top" Width="235"/>
        <Button x:Name="btnSubmit" Content="Submit" HorizontalAlignment="Left" Margin="352,161,0,0" VerticalAlignment="Top" Width="75" Click="btnSubmit_Click"/>

    </Grid>
</Window>
